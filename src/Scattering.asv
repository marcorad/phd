classdef Scattering < handle

    properties
        filterBanks %SFB filterbanks        
    end

    methods
        function obj = Scattering(Q, T, fs, N, flow, fhigh)
            fsfb = fs;
            for i = 1:numel(Q)
                obj.filterBanks(i) = SFB(Q(i), T, fsfb, N, flow(i), fs/2);
                fsfb = fsfb / obj.filterBanks(i) 
            end
        end
    end

end